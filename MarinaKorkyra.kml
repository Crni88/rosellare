<!-- Shared style for ALL berts -->
<Style id="bertStyle-normal">
  <LineStyle><color>ff7a6600</color><width>4</width></LineStyle>
  <PolyStyle><color>805c3f00</color></PolyStyle>
</Style>
<Style id="bertStyle-highlight">
  <LineStyle><color>ff7a6600</color><width>6</width></LineStyle>
  <PolyStyle><color>805c3f00</color></PolyStyle>
</Style>
<StyleMap id="bertStyle">
  <Pair><key>normal</key><styleUrl>#bertStyle-normal</styleUrl></Pair>
  <Pair><key>highlight</key><styleUrl>#bertStyle-highlight</styleUrl></Pair>
</StyleMap>

